<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
 
    <!-- consumer application name -->
    <dubbo:application name="consumer-of-helloworld-app"  />
    <!-- registry address, used for consumer to discover services -->
    <dubbo:registry address="zookeeper://192.168.1.241:2181" file="dubbo_ec_weichexin_cache" client="zkclient" check="false"/>
  
  	<!-- expose this service through dubbo protocol, through port 20880 -->
    <dubbo:protocol name="dubbo" port="20887"/>
	
    <dubbo:reference id="ecCarInfoService" interface="com.ecarinfo.core.dobbo.service.ECCarInfoService" check="false"/>
    
    <dubbo:reference id="esCarNoticeService" interface="com.ecarinfo.server.dobbo.service.ESCarNoticeService" check="false"/>
    
    <dubbo:reference id="eyProvinceAndCityService" interface="com.ecarinfo.yunying.service.EYProvinceAndCityService" check="false"/>
    
	<dubbo:reference id="trafficSearchApi" interface="com.ecarinfo.traffic.rpc.api.TrafficSearchApi" check="false"/>
 
</beans>