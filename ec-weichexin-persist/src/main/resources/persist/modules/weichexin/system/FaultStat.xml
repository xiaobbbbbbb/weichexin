<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.FaultStatDao">  
    
    <resultMap type="FaultStat" id="FaultStatResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="orgCode" column="org_code"/>
		<result 	property="carId" column="car_id"/>
		<result 	property="faultCode" column="fault_code"/>
		<result 	property="faultDetail" column="fault_detail"/>
		<result 	property="ctime" column="ctime"/>
		<result 	property="deviceNo" column="device_no"/>
		<result 	property="clickTime" column="click_time"/>
		<result 	property="orgId" column="org_id"/>
		<result 	property="groupId" column="group_id"/>
		<result 	property="groupCode" column="group_code"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM fault_stat WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM fault_stat WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM fault_stat WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM fault_stat WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="FaultStatResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM fault_stat WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM fault_stat        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="FaultStat" resultMap="FaultStatResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM fault_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM fault_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="FaultStat">
    	<![CDATA[ 
    		INSERT INTO fault_stat(org_code,car_id,fault_code,fault_detail,ctime,device_no,click_time,org_id,group_id,group_code) VALUES (#{orgCode},#{carId},#{faultCode},#{faultDetail},#{ctime},#{deviceNo},#{clickTime},#{orgId},#{groupId},#{groupCode})    		
        ]]> 
        <selectKey resultType="Long" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE fault_stat SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="FaultStat">
		<![CDATA[ 
			UPDATE fault_stat SET org_code = #{orgCode},car_id = #{carId},fault_code = #{faultCode},fault_detail = #{faultDetail},ctime = #{ctime},device_no = #{deviceNo},click_time = #{clickTime},org_id = #{orgId},group_id = #{groupId},group_code = #{groupCode} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="FaultStat">
		<![CDATA[ 
    		DELETE FROM fault_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Long">
		<![CDATA[ 
    		DELETE FROM fault_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM fault_stat WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  