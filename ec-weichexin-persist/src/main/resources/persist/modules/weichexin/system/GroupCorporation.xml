<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.GroupCorporationDao">  
    
    <resultMap type="GroupCorporation" id="GroupCorporationResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="groupName" column="group_name"/>
		<result 	property="groupCode" column="group_code"/>
		<result 	property="zone" column="zone"/>
		<result 	property="address" column="address"/>
		<result 	property="contactInfo" column="contact_info"/>
		<result 	property="ctime" column="ctime"/>
		<result 	property="comment" column="comment"/>
		<result 	property="isAway" column="isAway"/>
		<result 	property="delFlag" column="delFlag"/>
		<result 	property="updateTime" column="update_time"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
        	SELECT * FROM group_corporation WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
        	SELECT * FROM group_corporation WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
        	SELECT * FROM group_corporation WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM group_corporation WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM group_corporation WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
        	SELECT * FROM group_corporation        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="GroupCorporation" resultMap="GroupCorporationResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM group_corporation WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM group_corporation WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="GroupCorporation">
    	<![CDATA[ 
    		INSERT INTO group_corporation(group_name,group_code,zone,address,contact_info,ctime,comment,isAway,delFlag,update_time) VALUES (#{groupName},#{groupCode},#{zone},#{address},#{contactInfo},#{ctime},#{comment},#{isAway},#{delFlag},#{updateTime})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE group_corporation SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="GroupCorporation">
		<![CDATA[ 
			UPDATE group_corporation SET group_name = #{groupName},group_code = #{groupCode},zone = #{zone},address = #{address},contact_info = #{contactInfo},ctime = #{ctime},comment = #{comment},isAway = #{isAway},delFlag = #{delFlag},update_time = #{updateTime} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="GroupCorporation">
		<![CDATA[ 
    		DELETE FROM group_corporation WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM group_corporation WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM group_corporation WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  