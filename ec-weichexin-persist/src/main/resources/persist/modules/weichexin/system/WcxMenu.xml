<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.WcxMenuDao">  
    
    <resultMap type="WcxMenu" id="WcxMenuResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="type" column="type"/>
		<result 	property="menuKey" column="menu_key"/>
		<result 	property="name" column="name"/>
		<result 	property="ownId" column="own_id"/>
		<result 	property="level" column="level"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
        	SELECT * FROM wcx_menu WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
        	SELECT * FROM wcx_menu WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
        	SELECT * FROM wcx_menu WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM wcx_menu WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM wcx_menu WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
        	SELECT * FROM wcx_menu        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="WcxMenu" resultMap="WcxMenuResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM wcx_menu WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM wcx_menu WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="WcxMenu">
    	<![CDATA[ 
    		INSERT INTO wcx_menu(type,menu_key,name,own_id,level) VALUES (#{type},#{menuKey},#{name},#{ownId},#{level})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE wcx_menu SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="WcxMenu">
		<![CDATA[ 
			UPDATE wcx_menu SET type = #{type},menu_key = #{menuKey},name = #{name},own_id = #{ownId},level = #{level} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="WcxMenu">
		<![CDATA[ 
    		DELETE FROM wcx_menu WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM wcx_menu WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM wcx_menu WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  