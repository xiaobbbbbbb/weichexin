			UPDATE user_gindex SET car_id = #{carId},car_no = #{carNo},email = #{email},car_type = #{carType},pailiang = #{pailiang},oil_type = #{oilType},user_attr = #{userAttr},drive_num = #{driveNum},total_drive_time = #{totalDriveTime},total_mileage = #{totalMileage},total_oil_size = #{totalOilSize},total_oil_money = #{totalOilMoney},total_carbon_size = #{totalCarbonSize},total_carbon_money = #{totalCarbonMoney},avg_speed = #{avgSpeed},day_avg_oil = #{dayAvgOil},create_time = #{createTime},day_avg_carbon = #{dayAvgCarbon},day_carbon_money = #{dayCarbonMoney},client_day = #{clientDay} WHERE id = #{id}
