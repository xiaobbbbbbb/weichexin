    <select id="findByPK"  resultType="UserGindex" resultMap="UserGindexResultMap">  
        <![CDATA[ 
        	SELECT * FROM user_gindex WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="UserGindex" resultMap="UserGindexResultMap">  
        <![CDATA[ 
        	SELECT * FROM user_gindex WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="UserGindex" resultMap="UserGindexResultMap">  
        <![CDATA[ 
        	SELECT * FROM user_gindex WHERE `${fieldName}` = #{value}        	
        ]]>
