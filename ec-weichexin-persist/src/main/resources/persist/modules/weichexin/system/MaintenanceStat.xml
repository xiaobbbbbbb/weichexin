<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.MaintenanceStatDao">  
    
    <resultMap type="MaintenanceStat" id="MaintenanceStatResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="orgCode" column="org_code"/>
		<result 	property="carId" column="car_id"/>
		<result 	property="nextMileage" column="next_mileage"/>
		<result 	property="nextTime" column="next_time"/>
		<result 	property="currentMileage" column="current_mileage"/>
		<result 	property="deviceNo" column="device_no"/>
		<result 	property="ctime" column="ctime"/>
		<result 	property="orgId" column="org_id"/>
		<result 	property="groupId" column="group_id"/>
		<result 	property="groupCode" column="group_code"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM maintenance_stat WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM maintenance_stat WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM maintenance_stat WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM maintenance_stat WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM maintenance_stat WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM maintenance_stat        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="MaintenanceStat" resultMap="MaintenanceStatResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM maintenance_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM maintenance_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="MaintenanceStat">
    	<![CDATA[ 
    		INSERT INTO maintenance_stat(org_code,car_id,next_mileage,next_time,current_mileage,device_no,ctime,org_id,group_id,group_code) VALUES (#{orgCode},#{carId},#{nextMileage},#{nextTime},#{currentMileage},#{deviceNo},#{ctime},#{orgId},#{groupId},#{groupCode})    		
        ]]> 
        <selectKey resultType="Long" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE maintenance_stat SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="MaintenanceStat">
		<![CDATA[ 
			UPDATE maintenance_stat SET org_code = #{orgCode},car_id = #{carId},next_mileage = #{nextMileage},next_time = #{nextTime},current_mileage = #{currentMileage},device_no = #{deviceNo},ctime = #{ctime},org_id = #{orgId},group_id = #{groupId},group_code = #{groupCode} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="MaintenanceStat">
		<![CDATA[ 
    		DELETE FROM maintenance_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Long">
		<![CDATA[ 
    		DELETE FROM maintenance_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM maintenance_stat WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  