<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.FrontResourceDao">  
    
    <resultMap type="FrontResource" id="FrontResourceResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="name" column="name"/>
		<result 	property="code" column="code"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
        	SELECT * FROM front_resource WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
        	SELECT * FROM front_resource WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
        	SELECT * FROM front_resource WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM front_resource WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM front_resource WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
        	SELECT * FROM front_resource        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="FrontResource" resultMap="FrontResourceResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM front_resource WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM front_resource WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="FrontResource">
    	<![CDATA[ 
    		INSERT INTO front_resource(name,code) VALUES (#{name},#{code})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE front_resource SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="FrontResource">
		<![CDATA[ 
			UPDATE front_resource SET name = #{name},code = #{code} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="FrontResource">
		<![CDATA[ 
    		DELETE FROM front_resource WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM front_resource WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM front_resource WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  