<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.CarModelDao">  
    
    <resultMap type="CarModel" id="CarModelResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="name" column="name"/>
		<result 	property="code" column="code"/>
		<result 	property="year" column="year"/>
		<result 	property="serialId" column="serial_id"/>
		<result 	property="serialName" column="serial_name"/>
		<result 	property="oilSize" column="oil_size"/>
		<result 	property="oilNet" column="oil_net"/>
		<result 	property="oilStandard" column="oil_standard"/>
		<result 	property="pailiang" column="pailiang"/>
		<result 	property="url" column="url"/>
		<result 	property="issale" column="issale"/>
		<result 	property="disabled" column="disabled"/>
		<result 	property="oilChangeRate" column="oil_change_rate"/>
		<result 	property="mileageChangeRate" column="mileage_change_rate"/>
		<result 	property="avgOil" column="avg_oil"/>
		<result 	property="version" column="version"/>
		<result 	property="isNew" column="is_new"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_model WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_model WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_model WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM car_model WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="CarModelResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM car_model WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_model        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="CarModel" resultMap="CarModelResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM car_model WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM car_model WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="CarModel">
    	<![CDATA[ 
    		INSERT INTO car_model(name,code,year,serial_id,serial_name,oil_size,oil_net,oil_standard,pailiang,url,issale,disabled,oil_change_rate,mileage_change_rate,avg_oil,version,is_new) VALUES (#{name},#{code},#{year},#{serialId},#{serialName},#{oilSize},#{oilNet},#{oilStandard},#{pailiang},#{url},#{issale},#{disabled},#{oilChangeRate},#{mileageChangeRate},#{avgOil},#{version},#{isNew})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE car_model SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="CarModel">
		<![CDATA[ 
			UPDATE car_model SET name = #{name},code = #{code},year = #{year},serial_id = #{serialId},serial_name = #{serialName},oil_size = #{oilSize},oil_net = #{oilNet},oil_standard = #{oilStandard},pailiang = #{pailiang},url = #{url},issale = #{issale},disabled = #{disabled},oil_change_rate = #{oilChangeRate},mileage_change_rate = #{mileageChangeRate},avg_oil = #{avgOil},version = #{version},is_new = #{isNew} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="CarModel">
		<![CDATA[ 
    		DELETE FROM car_model WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM car_model WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM car_model WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  