<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.ServiceWorkerDao">  
    
    <resultMap type="ServiceWorker" id="ServiceWorkerResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="checkUserId" column="check_user_id"/>
		<result 	property="cuserId" column="cuser_id"/>
		<result 	property="name" column="name"/>
		<result 	property="checkTime" column="check_time"/>
		<result 	property="ctime" column="ctime"/>
		<result 	property="status" column="status"/>
		<result 	property="orgCode" column="org_code"/>
		<result 	property="orgId" column="org_id"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_worker WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_worker WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_worker WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM service_worker WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM service_worker WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_worker        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ServiceWorker" resultMap="ServiceWorkerResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM service_worker WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM service_worker WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="ServiceWorker">
    	<![CDATA[ 
    		INSERT INTO service_worker(check_user_id,cuser_id,name,check_time,ctime,status,org_code,org_id) VALUES (#{checkUserId},#{cuserId},#{name},#{checkTime},#{ctime},#{status},#{orgCode},#{orgId})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE service_worker SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="ServiceWorker">
		<![CDATA[ 
			UPDATE service_worker SET check_user_id = #{checkUserId},cuser_id = #{cuserId},name = #{name},check_time = #{checkTime},ctime = #{ctime},status = #{status},org_code = #{orgCode},org_id = #{orgId} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="ServiceWorker">
		<![CDATA[ 
    		DELETE FROM service_worker WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM service_worker WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM service_worker WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  