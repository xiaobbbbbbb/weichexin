<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.BackgroundUserDao">  
    
    <resultMap type="BackgroundUser" id="BackgroundUserResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="loginName" column="login_name"/>
		<result 	property="password" column="password"/>
		<result 	property="name" column="name"/>
		<result 	property="email" column="email"/>
		<result 	property="roleId" column="role_id"/>
		<result 	property="addTime" column="add_time"/>
		<result 	property="orgId" column="org_id"/>
		<result 	property="orgCode" column="org_code"/>
		<result 	property="groupId" column="group_id"/>
		<result 	property="groupCode" column="group_code"/>
		<result 	property="isAway" column="isAway"/>
		<result 	property="delFlag" column="delFlag"/>
		<result 	property="mark" column="mark"/>
		<result 	property="isReserveHandler" column="is_reserve_handler"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
        	SELECT * FROM background_user WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
        	SELECT * FROM background_user WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
        	SELECT * FROM background_user WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM background_user WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM background_user WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
        	SELECT * FROM background_user        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="BackgroundUser" resultMap="BackgroundUserResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM background_user WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM background_user WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="BackgroundUser">
    	<![CDATA[ 
    		INSERT INTO background_user(login_name,password,name,email,role_id,add_time,org_id,org_code,group_id,group_code,isAway,delFlag,mark,is_reserve_handler) VALUES (#{loginName},#{password},#{name},#{email},#{roleId},#{addTime},#{orgId},#{orgCode},#{groupId},#{groupCode},#{isAway},#{delFlag},#{mark},#{isReserveHandler})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE background_user SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="BackgroundUser">
		<![CDATA[ 
			UPDATE background_user SET login_name = #{loginName},password = #{password},name = #{name},email = #{email},role_id = #{roleId},add_time = #{addTime},org_id = #{orgId},org_code = #{orgCode},group_id = #{groupId},group_code = #{groupCode},isAway = #{isAway},delFlag = #{delFlag},mark = #{mark},is_reserve_handler = #{isReserveHandler} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="BackgroundUser">
		<![CDATA[ 
    		DELETE FROM background_user WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM background_user WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM background_user WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  