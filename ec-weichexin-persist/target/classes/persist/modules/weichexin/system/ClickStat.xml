<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.ClickStatDao">  
    
    <resultMap type="ClickStat" id="ClickStatResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="carId" column="car_id"/>
		<result 	property="deviceNo" column="device_no"/>
		<result 	property="sid" column="sid"/>
		<result 	property="type" column="type"/>
		<result 	property="clickTime" column="click_time"/>
		<result 	property="ctime" column="ctime"/>
		<result 	property="month" column="month"/>
		<result 	property="day" column="day"/>
		<result 	property="orgId" column="org_id"/>
		<result 	property="orgCode" column="org_code"/>
		<result 	property="groupId" column="group_id"/>
		<result 	property="groupCode" column="group_code"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM click_stat WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM click_stat WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM click_stat WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM click_stat WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="ClickStatResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM click_stat WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
        	SELECT * FROM click_stat        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ClickStat" resultMap="ClickStatResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM click_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM click_stat WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="ClickStat">
    	<![CDATA[ 
    		INSERT INTO click_stat(car_id,device_no,sid,type,click_time,ctime,month,day,org_id,org_code,group_id,group_code) VALUES (#{carId},#{deviceNo},#{sid},#{type},#{clickTime},#{ctime},#{month},#{day},#{orgId},#{orgCode},#{groupId},#{groupCode})    		
        ]]> 
        <selectKey resultType="Long" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE click_stat SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="ClickStat">
		<![CDATA[ 
			UPDATE click_stat SET car_id = #{carId},device_no = #{deviceNo},sid = #{sid},type = #{type},click_time = #{clickTime},ctime = #{ctime},month = #{month},day = #{day},org_id = #{orgId},org_code = #{orgCode},group_id = #{groupId},group_code = #{groupCode} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="ClickStat">
		<![CDATA[ 
    		DELETE FROM click_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Long">
		<![CDATA[ 
    		DELETE FROM click_stat WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM click_stat WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  