<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.ExportRecordDao">  
    
    <resultMap type="ExportRecord" id="ExportRecordResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="fileUrl" column="file_url"/>
		<result 	property="fileType" column="file_type"/>
		<result 	property="exportTime" column="export_time"/>
		<result 	property="exportUserId" column="export_user_id"/>
		<result 	property="orgId" column="org_id"/>
		<result 	property="orgCode" column="org_code"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
        	SELECT * FROM export_record WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
        	SELECT * FROM export_record WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
        	SELECT * FROM export_record WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM export_record WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM export_record WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
        	SELECT * FROM export_record        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ExportRecord" resultMap="ExportRecordResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM export_record WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM export_record WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="ExportRecord">
    	<![CDATA[ 
    		INSERT INTO export_record(file_url,file_type,export_time,export_user_id,org_id,org_code) VALUES (#{fileUrl},#{fileType},#{exportTime},#{exportUserId},#{orgId},#{orgCode})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE export_record SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="ExportRecord">
		<![CDATA[ 
			UPDATE export_record SET file_url = #{fileUrl},file_type = #{fileType},export_time = #{exportTime},export_user_id = #{exportUserId},org_id = #{orgId},org_code = #{orgCode} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="ExportRecord">
		<![CDATA[ 
    		DELETE FROM export_record WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM export_record WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM export_record WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  