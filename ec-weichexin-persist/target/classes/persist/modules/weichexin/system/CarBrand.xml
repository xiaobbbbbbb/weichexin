<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.ecarinfo.weichexin.dao.CarBrandDao">  
    
    <resultMap type="CarBrand" id="CarBrandResultMap">  
		<id 		property="id" column="id"/>
		<result 	property="code" column="code"/>
		<result 	property="name" column="name"/>
		<result 	property="letter" column="letter"/>
		<result 	property="img" column="img"/>
		<result 	property="disabled" column="disabled"/>
		<result 	property="changeRate" column="change_rate"/>
		<result 	property="version" column="version"/>
		<result 	property="isNew" column="is_new"/>
		 
    </resultMap>
    
    <select id="findByPK"  resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_brand WHERE id = #{id}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_brand WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_brand WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
        	SELECT ${selectFields} FROM car_brand WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="CarBrandResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM car_brand WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
        	SELECT * FROM car_brand        	
        ]]>
    </select>
    
    <select id="findList" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="CarBrand" resultMap="CarBrandResultMap">  
        <![CDATA[ 
      	 	SELECT ${selectFields} FROM car_brand WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <select id="count" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="long">  
        <![CDATA[ 
      	 	SELECT COUNT(id) FROM car_brand WHERE ${whereCond}      	 	
        ]]>
    </select>
    
    <insert id="insert" parameterType="CarBrand">
    	<![CDATA[ 
    		INSERT INTO car_brand(code,name,letter,img,disabled,change_rate,version,is_new) VALUES (#{code},#{name},#{letter},#{img},#{disabled},#{changeRate},#{version},#{isNew})    		
        ]]> 
        <selectKey resultType="Integer" order="AFTER" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID    
		</selectKey>
    </insert>
	
	<update id="updateWithCriteria" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
		UPDATE car_brand SET ${updateField} WHERE ${whereCond}	
		]]> 	
	</update>
	
	<update id="update" parameterType="CarBrand">
		<![CDATA[ 
			UPDATE car_brand SET code = #{code},name = #{name},letter = #{letter},img = #{img},disabled = #{disabled},change_rate = #{changeRate},version = #{version},is_new = #{isNew} WHERE id = #{id}
		]]> 	
	</update>
	
	<delete id="deleteEntity" parameterType="CarBrand">
		<![CDATA[ 
    		DELETE FROM car_brand WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="deleteByPK" parameterType="Integer">
		<![CDATA[ 
    		DELETE FROM car_brand WHERE id = #{id}    		
        ]]> 
	</delete>
	
	<delete id="delete" parameterType="com.ecarinfo.persist.criteria.Criteria">
		<![CDATA[ 
    		DELETE FROM car_brand WHERE ${whereCond}     		
        ]]> 
	</delete>
	
</mapper>  