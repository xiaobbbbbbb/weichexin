        	SELECT ${selectFields} FROM service_org WHERE ${uniqueWhereCond}        	
        ]]>
    </select>
    
    <select id="findMap" parameterType="com.ecarinfo.persist.criteria.Criteria" resultMap="ServiceOrgResultMap">  
        <![CDATA[ 
		    SELECT ${selectFields} FROM service_org WHERE ${whereCond}
        ]]>
    </select>
    
    <select id="findAll" resultType="ServiceOrg" resultMap="ServiceOrgResultMap">  
        <![CDATA[ 
