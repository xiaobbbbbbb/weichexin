    <select id="findByPK"  resultType="ServiceOrg" resultMap="ServiceOrgResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_org WHERE sid = #{sid}        	
        ]]>   
    </select>
    
    <select id="findOneByAttr" resultType="ServiceOrg" resultMap="ServiceOrgResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_org WHERE `${fieldName}` = #{value} limit 1        	
        ]]>
    </select>
    
    <select id="findByAttr" resultType="ServiceOrg" resultMap="ServiceOrgResultMap">  
        <![CDATA[ 
        	SELECT * FROM service_org WHERE `${fieldName}` = #{value}        	
        ]]>
    </select>
    
    <select id="findOne" parameterType="com.ecarinfo.persist.criteria.Criteria" resultType="ServiceOrg" resultMap="ServiceOrgResultMap">  
