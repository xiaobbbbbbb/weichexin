		<![CDATA[ 
			UPDATE service_org SET code = #{code},brands = #{brands},name = #{name},group_id = #{groupId},group_code = #{groupCode},logo = #{logo},address = #{address},tel = #{tel},ctime = #{ctime},mark = #{mark},start_time = #{startTime},end_time = #{endTime},is_valid = #{isValid},delFlag = #{delFlag},activity_publish_month = #{activityPublishMonth},activity_publish_num = #{activityPublishNum},activity_publish_max_num = #{activityPublishMaxNum},update_time = #{updateTime},tel_for_complaint = #{telForComplaint},tel_for_maintenance = #{telForMaintenance},tel_for_sales = #{telForSales},renew_insure_tel = #{renewInsureTel},used_car_serve_tel = #{usedCarServeTel},year_check_serve_tel = #{yearCheckServeTel},help_tel = #{helpTel},reserve_available_days = #{reserveAvailableDays},reserve_discount_note = #{reserveDiscountNote} WHERE sid = #{sid}
